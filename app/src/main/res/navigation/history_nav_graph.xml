<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/history_nav_graph"
    app:startDestination="@id/history_nav_host_fragment">


    <fragment
        android:id="@+id/history_nav_host_fragment"
        android:name="com.okurchenko.ecocity.ui.details.fragments.history.HistoryFragment"
        android:label="HistoryFragment"
        tools:layout="@layout/fragment_history">
        <action
            android:id="@+id/action_historyFragment_to_detailsFragment"
            app:destination="@id/details_nav_host_fragment" />
    </fragment>
    <fragment
        android:id="@+id/details_nav_host_fragment"
        android:name="com.okurchenko.ecocity.ui.details.fragments.details.DetailsFragment"
        android:label="DetailsFragment"
        tools:layout="@layout/fragment_details" />
</navigation>